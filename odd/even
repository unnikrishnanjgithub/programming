ackage oe;
import java.util.*;
public class OE1 {

	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("ente the number");
		int a=s.nextInt();
		if(a%2==0)
		{
			System.out.println("even num");
		}
		else
		{
			System.out.println("odd num");
		}
		

	}

}
