package factorial;
import java.util.*;
public class Fact1 
{
 void display(int n)
 {
	int f=1;
	while(n>0)
	{
		f=f*n;
		n--;
	}
	System.out.println("the factorial of a number is"+f);
 }
	public static void main(String[] args) 
	{
	Fact1 s1=new Fact1();
	 Scanner s=new Scanner(System.in);
	 System.out.println("enter the number");
	 int x=s.nextInt();
	 s1.display(x);

	}
	

}
